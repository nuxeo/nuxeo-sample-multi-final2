<package type="addon" name="nuxeo-sample-final2-package" version="@VERSION@">
  <title>Sample Final2 Package</title>
  <description>
    <p>Sample Final2 Package for SUPINT-1828 Tests</p>
  </description>
  <vendor>Nuxeo</vendor>
  <installer restart="true" />
  <uninstaller restart="true" />
  <nuxeo-validation>none</nuxeo-validation>
  <production-state>testing</production-state>
  <supported>false</supported>
  <!-- needed for private package selection on Studio side -->
  <platforms>
    <platform>@NUXEO_TARGET_PLATFORM@</platform>
  </platforms>
  <license />
  <license-url />
  <visibility>DEV</visibility>
  <dependencies>
    <!-- no dependencies except web-ui and common: other deps embedded in the package for this sample -->
    <package>nuxeo-web-ui</package>
    <package>nuxeo-sample-common-package:@SAMPLE_COMMON_PACKAGE_VERSION@</package>
  </dependencies>
</package>
